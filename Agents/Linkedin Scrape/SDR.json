    {
      "parameters": {
        "jsCode": "// Get data from previous node\nconst agentOutput = $input.all();\nconst outputString = agentOutput[0].json.output;\n\n// Clean and parse the JSON\nconst jsonString = outputString.replace(/```json\\n?|```/g, '').replace(/json\\n?|/g, '').trim();\nconst data = JSON.parse(jsonString);\n\n// Extract fields\nconst companyName = data.company_name;\nconst location = data.location;\nconst role = data.role;\nconst clientStatus = data.client_status;\n\n// Return for next node\nreturn [\n    {\n        json: {\n            company_name: companyName,\n            location: location,\n            role: role,\n            client_status: clientStatus,\n        }\n    }\n];\n"
      },
